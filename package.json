{
  "name": "geenie-proxy",
  "version": "0.1.0",
  "description": "Geenie MCP proxy server - authenticates and routes requests to Amazon Ads MCP",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:mock": "tsx watch tests/mocks/amazon-server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "tsc --noEmit"
  },
  "keywords": ["mcp", "proxy", "amazon-ads", "geenie"],
  "author": "Geenie.io",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@supabase/supabase-js": "^2.47.10",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "node-cache": "^5.1.2",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "rate-limiter-flexible": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
